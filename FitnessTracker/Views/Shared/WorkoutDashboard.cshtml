<style>
    .container3 {
        position: absolute;
        width: 409px;
        height: 800px;
        margin-bottom: 100px;
        background-color: white;
        border-radius: 25px;
        margin-left: 10px;
    }
</style>
<div class="container3"></div>
<div class="limiter">
    <div style="margin-left:10px">
        <div class="wrap-table100">
            <div class="table100">
                <table class="tableWorkouts">
                    <thead>
                        <tr class="table100-head">
                            <th style="width: 60%">Name</th>
                            <th style="width: 40%">Count of exercises</th>
                        </tr>
                    </thead>
                    @foreach (var workout in Model.userWorkouts)
                    {
                        <tr class="backgroundColorWorkout">
                            <td style="width: 60%">@workout.Name</td>
                            <td style="width: 40%;">@workout.ExerciseCount</td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>
</div>
